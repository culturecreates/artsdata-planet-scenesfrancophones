        "image": {
          "@type": "ImageObject",
          "representativeOfPage": "True",
          "url": "https://scenesfrancophones.ca/sites/default/files/styles/thumbnail_430/public/ajouter_un_evenement-2025/Francois-Massicotte-affiche-carre%CC%81e.jpg?itok=CnAOozpF",
          "width": "430",
          "height": "430"
