        "name": "Michelle Desrochers -",
        "eventStatus": "https://schema.org/EventScheduled",
        "image": {
          "@type": "ImageObject",
          "representativeOfPage": "True",
          "url": "https://scenesfrancophones.ca/sites/default/files/styles/thumbnail_430/public/ajouter_un_evenement-2025/michelle%20desrochers.jpg?itok=QbHOxfic",
