        "name": "Jeunesses Musicales Canada - Bal Acadjonne",
        "eventStatus": "https://schema.org/EventScheduled",
        "image": {
          "@type": "ImageObject",
          "representativeOfPage": "True",
          "url": "https://scenesfrancophones.ca/sites/default/files/styles/thumbnail_430/public/2025-06/jmus_creleve25-26_aff_11x18_web_lakme.jpg?itok=6YMoCWzO",
          "width": "430",
