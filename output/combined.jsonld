        "image": {
          "@type": "ImageObject",
          "representativeOfPage": "True",
          "url": "https://scenesfrancophones.ca/sites/default/files/styles/thumbnail_430/public/ajouter_un_evenement-2025/NS24-25_Crawlspace_Instagram_Post.jpg?itok=i5nRtp-M",
          "width": "430",
