    "@graph": [
      {
        "@type": "Event",
        "url": "https://scenesfrancophones.ca/spectacles/flammeche-theatre-cet-ete-qui-chantait-8464",
        "name": "Flammèche Théâtre -",
        "eventStatus": "https://schema.org/EventScheduled",
        "image": {
          "@type": "ImageObject",
          "representativeOfPage": "True",
          "url": "https://scenesfrancophones.ca/sites/default/files/styles/thumbnail_430/public/ajouter_un_evenement-2025/cet%20%C3%A9t%C3%A9%20qui%20chantait.jpeg?itok=H58X7q8X",
          "width": "430",
          "height": "430"
        },
        "eventAttendanceMode": "https://schema.org/OfflineEventAttendanceMode",
        "description": "Une corneille, portée par la brise,\r\nsurvole les montagnes, la vallée et enfin le fleuve.\r\nElle vous guide jusqu'à la petite maison aux volets verts,\r\ntout au bout de la route de Petite-Rivière-Saint-François.\r\nLà, vous trouvez Gabrielle observant de ses yeux brillants\r\ntoute la vie dans ce coin de pays :\r\nle petit foin qui danse dans le vent ;\r\nles fleurs qui foisonnent sur un tas de fumier ;\r\nles oiseaux blancs et noirs longeant les battures\r\nqui crient à la marée basse ;\r\nles lucioles chuchotant à la tombée de la nuit.\r\n\r\n\r\n\r\nLibrement inspiré du livre éponyme de Gabrielle Roy, Cet été qui chantait est un spectacle tout en images, en musique et en douceur.\r\n\r\n\r\nMarie-Ève Fontaine et son équipe recréent l'univers profondément poétique de l'autrice franco-manitobaine dans un théâtre d'images et vous invitent à faire un voyage sensoriel destiné à tous les publics, tout spécialement les petits et les aînés.",
        "startDate": "2025-04-06T15:00:00-04:00",
        "isAccessibleForFree": "True",
